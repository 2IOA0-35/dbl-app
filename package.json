{
    "name": "dbl-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "antd": "^4.15.3",
        "craco-less": "^1.17.1",
        "d3": "^6.7.0",
        "dom-to-image": "^2.6.0",
        "moment": "^2.29.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "serve": "^11.3.2",
        "web-vitals": "^1.1.1",
        "worker-loader": "^3.0.8"
    },
    "devDependencies": {
        "eslint": "^7.24.0",
        "eslint-plugin-react": "^7.23.2"
    },
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "serve": "serve -s build",
        "lint": "eslint ."
    },
    "eslintConfig": {
        "ignorePatterns": [
            "build/**/*",
            "public/**/*"
        ],
        "env": {
            "browser": true,
            "es2021": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended"
        ],
        "parserOptions": {
            "ecmaFeatures": {
                "jsx": true
            },
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "plugins": [
            "react"
        ],
        "rules": {
            "quotes": [
                1,
                "single",
                {
                    "avoidEscape": true
                }
            ],
            "jsx-quotes": [
                1,
                "prefer-single"
            ],
            "indent": [
                1,
                4,
                {
                    "SwitchCase": 1
                }
            ],
            "semi": "warn",
            "react/react-in-jsx-scope": 0,
            "no-unused-vars": 1
        },
        "settings": {
            "react": {
                "version": "detect"
            }
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
